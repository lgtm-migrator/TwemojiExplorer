{
  "name": "twemoji-explorer",
  "version": "0.0.1",
  "description": "Website for exploring emojis.",
  "private": "true",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rm -rf ./public",
    "build": "npm run clean && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "NODE_ENV=development rollup  -c -w --no-watch.clearScreen",
    "serve": "npm run clean && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/runtime": "^7.14.0",
    "@elderjs/elderjs": "^1.4.13",
    "@elderjs/plugin-browser-reload": "^1.0.0",
    "@elderjs/plugin-images": "^1.0.2",
    "autoprefixer": "^10.2.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "emojilib": "^3.0.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "flexsearch": "^0.6.32",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "intersection-observer": "^0.12.0",
    "nodemon": "^2.0.7",
    "polka": "^0.5.2",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.3.0",
    "rollup": "^2.51.0",
    "sirv": "^1.0.12",
    "svelte": "3.38.2",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.2.0",
    "twemoji-parser": "^14.0.0",
    "typescript": "^4.3.2",
    "unicode-emoji-json": "^0.3.1",
    "vanilla-lazyload": "^17.4.0"
  }
}
