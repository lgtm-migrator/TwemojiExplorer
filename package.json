{
  "name": "twemoji-explorer",
  "version": "0.0.1",
  "description": "Website for exploring emojis.",
  "private": "true",
  "main": "./src/build.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rm -rf ./public",
    "build": "npm run clean && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "NODE_ENV=development rollup  -c -w --no-watch.clearScreen",
    "serve": "npm run clean && npm run build:rollup && NODE_ENV=production node ./src/server.js",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@elderjs/elderjs": "^1.4.3",
    "@elderjs/plugin-browser-reload": "^0.1.37",
    "@elderjs/plugin-seo-check": "0.1.37",
    "autoprefixer": "^10.2.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "intersection-observer": "^0.12.0",
    "nodemon": "^2.0.7",
    "polka": "^0.5.2",
    "postcss": "^8.2.8",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.41.1",
    "sirv": "^1.0.11",
    "svelte": "3.35.0",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
